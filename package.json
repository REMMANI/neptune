{
  "name": "neptune",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "prepare": "husky install",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:seed": "tsx prisma/seed.ts",
    "pack:cpanel": "rm -rf deploy && mkdir -p deploy/.next && npm run build && cp -r .next/standalone deploy/.next/standalone && cp -r .next/static deploy/.next/static && if [ -d public ]; then cp -r public deploy/public; fi && node -e \"require('fs').writeFileSync('deploy/app.js', 'process.env.HOST=\\'0.0.0.0\\';process.env.PORT=process.env.PORT||\\'3000\\';require(\\'./.next/standalone/server.js\\')')\" && if [ -f .env.production ]; then cp .env.production deploy/.env; elif [ -f .env.local ]; then cp .env.local deploy/.env; fi"
  },
  "dependencies": {
    "@prisma/client": "^6.17.0",
    "@prisma/extension-accelerate": "^2.0.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "ioredis": "^5.4.1",
    "lucide-react": "^0.545.0",
    "next": "15.5.4",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^2.2.0",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/css-modules": "^1.0.5",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lighthouse": "^12.8.2",
    "lint-staged": "^16.2.3",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.17.0",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  }
}
